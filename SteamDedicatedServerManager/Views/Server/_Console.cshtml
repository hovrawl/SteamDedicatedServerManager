@model ServerDetailsViewModel
@{
    var server = Model.ServerInstance;
    var serverId = server?.Id.ToString() ?? "";
}
<button class="h-connect-console-btn  btn btn-primary" type="button" data-id="@serverId">Connect Console</button>

<div class="container d-flex flex-column flex-grow-1 h-server-console overflow-auto" id="server-console">
    <div class="h-server-console-container overflow-auto">
        <span class="h-server-messages d-flex flex-column overflow-auto"></span>
    </div>
</div>
